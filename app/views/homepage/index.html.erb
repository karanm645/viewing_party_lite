<% if current_user.nil? %>
  <%= link_to "go to login", "/login" %>
  <%= button_to "Create a New User", "/register", method: :get %>
<% else %>
  <%= link_to "logout", "/logout", method: :destroy %>
  <h3> Existing Users </h3>
  <% @users.each do |user| %>
  <li><%= link_to "#{user.email}'s Dashboard", user_path(user) %></li>
  <% end %>
<% end %>